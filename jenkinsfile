pipeline {
    agent any
    stages{
        stage ('Checkout')
        {
            steps {
                git branch: 'main', url: 'https://github.com/Suresh8904/newrep.git'
            }
        }
    }
    stages{
        stage ('Build')
        {
            steps {
                sh 'echo "building"'
            }
        }
        stage ('Test')
        {
            steps {
                sh 'echo "testing"'
            }
        }
        stage ('Deploy')
        {
            steps {
                sh 'echo "deploy"'
            }
        }
    }
}